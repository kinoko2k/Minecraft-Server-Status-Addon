{
    "manifest_version": 2,
    "name": "Minecraft Server Status Checker",
    "description": "マイクラサーバーのIPアドレスを検索できます。",
    "version": "1.2",
    "browser_specific_settings": {
        "gecko": {
            "id": "MCStatusAddon@kinoko1216"
        }
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "32": "icons/icon.svg"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icons/icon.svg"
        }
    }
}